<package type="addon" name="nuxeo-platform-user-registration" version="@VERSION@">
  <title>Nuxeo Platform User Registration</title>
  <description>
    <p>Nuxeo Platform User Registration allows you to invite external people to access a specific Workspace / Folder to work in with specified rights.</p>
  </description>
  <home-page>https://doc.nuxeo.com/nxdoc/nuxeo-platform-user-registration/</home-page>
  <vendor>Nuxeo</vendor>
  <installer restart="false" />
  <uninstaller restart="true" />
  <!-- <hotreload-support>true</hotreload-support> -->
  <!-- <require-terms-and-conditions-acceptance>false -->
  <!-- </require-terms-and-conditions-acceptance> -->
  <nuxeo-validation>nuxeo_certified</nuxeo-validation>
  <production-state>production_ready</production-state>
  <supported>true</supported>
  <platforms>
    <platform>server-@DISTRIB_VERSION@</platform>
    <platform>server-@DISTRIB_VERSION@-*</platform>
  </platforms>
  <dependencies>
    <package>nuxeo-jsf-ui</package>
  </dependencies>
  <conflicts>
    <package>nuxeo-cmf</package>
  </conflicts>
  <license>LGPL</license>
  <license-url>http://www.gnu.org/licenses/lgpl.html</license-url>
  <visibility>PUBLIC</visibility>
</package>
